<%= link_to "Compassion for Humanity", root_path, :class => 'brand' %>
<ul class="nav"></ul>
  <div id="session_btns" class="pull-right">
    <% if user_signed_in? %>
    <div class="nav_profile_img pull-left">
      <img src="<%= facebook_image(20, 20) %>" alt="" class="thumbnail">
    </div>
    <%= current_user.name %>
    <%= link_to 'Logout', destroy_user_session_path, :method=>'delete' %>
    <div class="pull-right dropdown">
      <a id="dLable" class="dropdown-toggle" role="button" data-toggle="dropdown" data-target="#" href="#">Account <b class="caret"></b></a>
      <ul id="nav-dropdown" class="dropdown-menu">
        <li><%=link_to "Profile", user_path(current_user) %></li>
        <li><%=link_to "Create New Project", new_user_project_path(current_user) %></li>
        <li><%=link_to "Your Projects", user_projects_path(current_user) %></li>
      </ul>
    </div>
  <% else %>
  <a class="btn btn-success pull-left" href="#facebook_sign_in" "data-toggle"="modal">Sign Up</a>
  <% end %>
  </div>