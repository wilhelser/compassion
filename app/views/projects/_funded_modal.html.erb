<div class="modal hide fade" id="funded_modal">
  <div class="modal-header">
    <h3>Your Action has been funded!</h3>
  </div>
  <div class="modal-body">
        <p>What do you want to do?</p>
      <div class="clearfix"></div>
  </div><!-- modal-body -->
  <div class="modal-footer">
    <%= link_to "End Campaign", end_campaign_project_path(@project), :class => "btn btn-large btn-danger", "data-disable-with" => "Ok..." %>
    <button id="continue-campaign" class="btn btn-large btn-primary">Keep it Going</button>
  </div>
</div>
<script>
  $(document).ready(function() {
    $("#funded_modal").modal('show');
  });
</script>
