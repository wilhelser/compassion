<% if @estimate.errors.any? %>
$("#estimate-errors-wrap").html('<%= escape_javascript(render("estimates/error_messages", :formats => [:html])) %>').removeClass('hide');
<% else %>
  $("#adjuster_estimates_wrap").html("<%= j render @estimates %>");
  $("#edit_estimate_modal").modal('hide');
  $("#edit-estimate-form-wrap").html("<%= j render 'estimates/edit_form' %>");
  $('#assignments-wrap').html("<%= j render @assignments %>");
<%  end %>
